{{#extend "base"}}
  {{#content "head"}}
    {{> head-partials/page-title }}
  {{/content}}
  {{#content "main"}}
    {{#> modal-partials/modal}}
      <h1 class="text text_size_m text_weight_bold">{{pages.h1}}</h1>
      <nav>
        <ul>
          {{#each pages.links}}
            <li><a href={{pageUrl}}>
            {{> form/input-field/input-field }}
            </a></li>
          {{/each}}
        </ul>
      </nav>
   
    {{/modal-partials/modal}}
  {{/content}}
{{/extend}}


