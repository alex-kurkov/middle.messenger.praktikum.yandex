<input 
  class="input text text_size_m text_weight_regular {{class}}"
  type={{type}}
  value="{{value}}"
  name="{{name}}"
  placeholder="{{placeholder}}"
  {{#if error}}data-error="true"{{/if}}
  {{#if autocomplete}}autocomplete{{else}}autocomplete="off"{{/if}}
  id="{{id}}"
  accept="{{accept}}"
/>
