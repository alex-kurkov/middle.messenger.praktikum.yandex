<div class="chat-messages flex-vertical-left">
  {{#if chatMessages}}
    <ul class="chat-messages__messages flex-vertical-left">
      {{#each chatMessages as |message| }}
        <li class="chat-messages__list-item">
          {{{FeedMessage message=message }}}
        </li>
      {{/each}}
    </ul>
    {{{NewMessage inputs=inputs}}}
  {{/if}}
</div>
